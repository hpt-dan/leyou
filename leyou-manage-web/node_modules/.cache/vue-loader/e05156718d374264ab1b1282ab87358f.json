sponse.data.items;\n          this.loading = false;\n        }).catch(error => {\n          console.log(error);\n          this.loading = false;\n        });\n        this.brands = [\n          {id: 1, name: \"小米\", image: \"1.jpg\", letter: \"X\"},\n          {id: 2, name: \"大米\", image: \"1.jpg\", letter: \"D\"},\n          {id: 3, name: \"超大米\", image: \"1.jpg\", letter: \"C\"},\n          {id: 4, name: \"非常大米\", image: \"1.jpg\", letter: \"F\"},\n        ]\n      }\n    }\n  }\n</script>\n\n<style scoped>\n\n</style>\n"]}]}